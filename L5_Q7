//. Fa√ßa um algoritmo que gere e escreva automaticamente a seguinte matriz:
//1 1 1 1 1 1
//1 2 2 2 2 1
//1 2 3 3 2 1
//1 2 3 3 2 1
//1 2 2 2 2 1
//1 1 1 1 1 1 

  
#include <iostream>
using namespace std;
int main() {
  int M[6][6], i, j;

  for(i=0; i<6; i++){
    for(j=0; j<6; j++){
        M[i][j] = 1;
      }
  }
  for(i=1; i<5; i++){
    for(j=1; j<5; j++){
        M[i][j] = 2;
      }
    }
  for(i=2; i<4; i++){
    for(j=2; j<4; j++){
        M[i][j] = 3;
    }
  }

    for(i=0; i<6; i++){
      for(j=0; j<6; j++){
        cout<<M[i][j]<<" ";
    }
    cout<<"\n"<<endl;
  }

}
